<h1>Please Login Below</h1>






<%if flash[:message].present?%>
<div id="error_explanation">
<%= flash[:message] %>
</div><%end%>


<%if !logged_in?%>
<%=form_tag sessions_path do%>
<%=label_tag :username%>
<%=text_field_tag :username%><br>
<%=label_tag :password%>
<%=password_field_tag :password%><br>

<%= submit_tag "Submit"%>
<%end%><%end%>
<%=link_to "Back to Signup", '/'%>